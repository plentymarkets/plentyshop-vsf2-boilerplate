{
  "name": "@vue-storefront/plentymarkets-theme",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=14.17.x"
  },
  "scripts": {
    "build": "nuxt build -m",
    "build:analyze": "nuxt build -a -m",
    "dev": "./build-env.sh && nuxt",
    "generate": "nuxt generate",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "precommit": "lint-staged",
    "start": "nuxt start",
    "test": "jest",
    "test:e2e": "cypress open --config-file tests/e2e/cypress.config.js",
    "test:e2e:generate:report": "yarn -s mochawesome-merge \"tests/e2e/report/*.json\" > \"tests/e2e/report.json\" && yarn -s marge tests/e2e/report.json -o \"tests/e2e/report\"",
    "test:e2e:hl": "cypress run --headless --config-file tests/e2e/cypress.config.js",
    "update:check": "ncu",
    "update:update": "ncu -u",
    "dev-debug": "node --inspect node_modules/.bin/nuxt"
  },
  "dependencies": {
    "@storefront-ui/vue": "0.13.6",
    "@vue-storefront/middleware": "~2.7.5",
    "@vue-storefront/nuxt": "~2.7.5",
    "@vue-storefront/nuxt-theme": "~2.7.5",
    "@vue-storefront/plentymarkets": "0.0.1",
    "cookie-universal-nuxt": "^2.1.5",
    "@nuxtjs/google-fonts": "^2.0.0",
    "core-js": "^3.19.0",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.5.0",
    "vee-validate": "^3.4.13",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@vue/test-utils": "^1.2.2",
    "babel-jest": "^27.3.1",
    "cypress": "^10.9.0",
    "cypress-pipe": "^2.0.0",
    "cypress-tags": "^0.3.0",
    "jest": "^27.5.1",
    "mochawesome": "^6.3.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "ts-loader": "^8.1.0",
    "vue-jest": "^4.0.0-0"
  }
}
